FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "5";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 6.1e6;

boundaryField
{
    inlet
    {
        type            fixedFluxPressure;
    }
    outlet
    {
        type            prghPressure;
        p               $internalField;
        value           $internalField;
    }
    wall
    {
        type            fixedFluxPressure;
    }
    front
    {
        type            fixedFluxPressure;
    }
    back
    {
        type            fixedFluxPressure;
    }
    axis
    {
        type            symmetry;
    }
}

// ************************************************************************* //
